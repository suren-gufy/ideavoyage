{
  "version": 2,
  "buildCommand": "cd IdeaVoyage/IdeaVoyage && npm install && npm run vercel-build",
  "outputDirectory": "IdeaVoyage/IdeaVoyage/dist/public",
  "functions": {
    "IdeaVoyage/IdeaVoyage/api/*.ts": { "runtime": "@vercel/node@2.x" }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/IdeaVoyage/IdeaVoyage/api/index.ts" },
    { "src": "/(.*)", "dest": "/IdeaVoyage/IdeaVoyage/dist/public/index.html" }
  ]
}